<div class="row" style="margin-top: 60px;">
  <div class="col-md-2"></div>
  <div class="col-md-6">
    <h1 class="text-center" style="margin: 20px;"><%= @registered_app.name %></h1>
    <% if @events.count > 0 %>
       <table class="table table-striped" style="margin: 40px;">
          <thead style="margin: 20px;">
            <tr>
              <th>Event Name</th>
              <th>Number of Events</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <% @events.each do |event_name, event_arr| %>
                  <td><%= event_name %></td>
                  <td><%= event_arr.count %></td>
                  <!-- <td><%= line_chart event_arr.group_by_day(:created_at).count %></td> -->
               <% end %>
           </tr>
         </tbody>
       </table>
   <% else %>
     <h5 class="text-center" style="margin: 20px;">No Events received yet!</h5>
   <% end %>
  </div>
</div>
